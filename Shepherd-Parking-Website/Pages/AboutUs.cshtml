@page
@model AboutUsModel

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Meet The Team</title>
    <style>
        body {
            font-family: Arial, sans-serif;
        }
        h1 {
            text-align: center;
            color: #008000;
        }
        .team-container {
            display: grid;
            grid-template-columns: repeat(3, 1fr);
            grid-gap: 20px;
            padding-left: 60px;
        }
        .team-member {
            text-align: center;
            position: relative;
            transition: transform 0.3s ease-in-out;
            cursor: pointer;
        }
        .team-member img {
            width: 135px;
            height: 135px;
            border-radius: 50%;
            transition: opacity 0.3s ease-in-out;
        }
        .team-member-description {
            position: absolute;
            top: 0;
            left: 50%;
            transform: translateX(-50%);
            width: 100%;
            padding: 10px;
            background-color: rgba(0, 128, 0, 0.8);
            color: white;
            text-align: center;
            opacity: 0;
            visibility: hidden;
            transition: opacity 0.3s ease-in-out, visibility 0.3s ease-in-out;
            border-radius: 5px;
        }
        .team-member:hover img {
            opacity: 0;
        }
        .team-member:hover .team-member-description {
            opacity: 1;
            visibility: visible;
        }
        .team-member-name {
            margin-top: 10px;
            color: #000;
        }
    </style>
</head>
<body>

<h1>Meet The Team</h1>

<div class="team-container">
    <div class="team-member">
        <img src="data:image/jpeg;base64," alt="Aron">
        <div class="team-member-name">Aron</div>
        <div class="team-member-description">Lead Developer with expertise in front-end technologies.</div>
    </div>
    
    <div class="team-member">
        <img src="data:image/jpeg;base64," alt="Brandon">
        <div class="team-member-name">Brandon</div>
        <div class="team-member-description">Back-end Developer specializing in databases and APIs.</div>
    </div>

    <div class="team-member">
        <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTvzw2vQRjRekgXEwvjOSq2sJY1iVgXV9WrGQ&amp;s" alt="Quinton">
        <div class="team-member-name">Quinton</div>
        <div class="team-member-description">UX/UI Designer focused on creating user-friendly interfaces.</div>
    </div>
    
    <div class="team-member">
        <img src="https://image.civitai.com/xG1nkqKTMzGDvpLrqFT7WA/8a09460e-8e2f-4e01-b5bc-eb237618c55a/width=450/Sam_Hyde-_The_Candyman_20240101033414_e000007_02.jpeg" alt="Ross">
        <div class="team-member-name">Ross</div>
        <div class="team-member-description">DevOps Engineer managing cloud infrastructure and CI/CD.</div>
    </div>

    <div class="team-member">
        <img src="https://static.truckersmp.com/avatarsN/1521793.1581289063.jpg" alt="Rylan">
        <div class="team-member-name">Rylan</div>
        <div class="team-member-description">Security Specialist ensuring safe and secure web applications.</div>
    </div>

<div class="team-member">
        <img src="data:image/jpeg;base64," alt="Shavir">
        <div class="team-member-name">Shavir</div>
        <div class="team-member-description">QA Engineer focusing on testing and quality assurance.</div>
    </div>
</div>

</body>
</html>
